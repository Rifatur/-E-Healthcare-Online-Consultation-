@model IEnumerable<Hoyna.Models.Employee>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout/_LayoutAdmin.cshtml";
}
@{
    <link rel="stylesheet" href="~/Context/css/Employee.css">
}



<div class="navicationBar">
    <div class="Addbtn"><a href="~/Employee/Upload">Add Employee</a></div>
    <div class="Addbtn"><a href="~/Employee/Employeetype">Add Employee Type</a></div>
</div>

<table class="table">
    <tr>

        <th>
            Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Gander)
        </th>
        <th>
            Action
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.EmpFName) @Html.DisplayFor(modelItem => item.EmpLName)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gander)
            </td>
            <td>
                @Html.ActionLink("View", "Details", "Employee", new { EmpId = @item.EmpId }, new { })
                @Html.ActionLink("Edit", "Edit", "Employee", new { EmpId = @item.EmpId }, new { })
                @Html.ActionLink("Delete", "Delete", "Employee", new { EmpId = @item.EmpId }, new { })
            </td>

        </tr>
    }

</table>





