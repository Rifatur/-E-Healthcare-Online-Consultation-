@model Hoyna.Models.DoctorModel
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Layout/_LayoutAdmin.cshtml";
}

    <table id="customers">
        <tr>
            <td>Doctor Id</td>
            <td>@Model.DocId</td>
        </tr>
        <tr>
            <td>Doctor Name</td>
            <td>@Model.FName @Model.LName</td>
        </tr>
        <tr>
            <td>Email</td>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <td>Password</td>
            <td>@Model.Passward</td>
        </tr>
        <tr>
            <td>Date of Birth </td>
            <td>@Model.DOB</td>
        </tr>
        <tr>
        <tr>
            <td>Gander</td>
            <td>@Model.Gander</td>
        </tr>
        <tr>
        <tr>
            <td>Address</td>
            <td>@Model.DocAddress</td>
        </tr>
        <tr>
        <tr>
            <td>Country </td>
            <td>@Model.Country</td>
        </tr>
        <tr>
        <tr>
            <td>City </td>
            <td>@Model.City</td>
        </tr>
        <tr>
            <td>State </td>
            <td>@Model.State</td>
        </tr>
        <tr>
            <td>Post </td>
            <td>@Model.Post</td>
        </tr>
        <tr>
            <td>Phone No </td>
            <td>@Model.PhoneNo</td>
        </tr>
        <tr>
            <td>Department Name </td>
            <td>@Model.DName</td>
        </tr>
        <tr>
            <td>Created Date</td>
            <td>@Model.CreatedDate</td>
        </tr>
        <tr>
            <td>
                @Html.ActionLink("Back", "index", "Doctor", new { })
            </td>
        </tr>

    </table>

