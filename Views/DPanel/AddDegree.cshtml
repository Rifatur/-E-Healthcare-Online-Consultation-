
@{
    ViewBag.Title = "AddDegree";
    Layout = "~/Views/Layout/_LayoutDpanel.cshtml";
}

    <div class="card-form">
        <div class="signup">
            @using (Html.BeginForm("AddDegree", "DPanel", FormMethod.Post))
            {

                <div class="form-title">Add You Education</div>
                <div class="form-body">
                    <div class="row">
                        <input type="text" placeholder="Graduation Name*" name="DegreeName">
                      
                    </div>
                    <div class="row">
                        <input type="date" placeholder="First Name*" name="StartDate">
                        <input type="date" placeholder="Last Name*" name="EndDate">
                    </div>
                    <div class="row">
                        <input type="text" placeholder="University Name*" name="University">
                    </div>
                    <div class="row">
                        <input type="text" placeholder="Country *" name="Country">
                        <input type="text" placeholder="City *" name="City">
                    </div>
                    <div class="row">
                        <input type="text" placeholder="Doctor Id*" name="DocId" value="@Session["DocId"].ToString()">
                    </div>
                </div>

                <input type="submit" value="Submit" />
            }
        </div>
    </div>

